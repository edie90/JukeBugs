  -o-animation: bubbleshow 0.5s;
  animation: bubbleshow 0.5s;
}
.kerve .spikewrap .bubble.hide {
  filter: alpha(opacity=100);
  -khtml-opacity: 1;
  -moz-opacity: 1;
  opacity: 1;
}
.kerve .spike_details {
