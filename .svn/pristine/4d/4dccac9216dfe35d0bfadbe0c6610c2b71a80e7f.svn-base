  -ms-transform: translateZ(0);
  transform: translateZ(0);
}
.kerve .spikeartist.animated {
  /*-webkit-transition: all 400ms cubic-bezier(0.680, -0.550, 0.2, 1.550); 
	-moz-transition: all 400ms cubic-bezier(0.680, -0.550, 0.2, 1.550); 
