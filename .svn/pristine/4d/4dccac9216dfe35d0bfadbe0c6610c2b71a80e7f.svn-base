  -moz-opacity: 1;
  opacity: 1;
}
.kerve .spikewrap .bubble.show {
  filter: alpha(opacity=100);
  -khtml-opacity: 1;
  -moz-opacity: 1;
  opacity: 1;
  -webkit-animation: bubbleshow 0.5s;
  -moz-animation: bubbleshow 0.5s;
