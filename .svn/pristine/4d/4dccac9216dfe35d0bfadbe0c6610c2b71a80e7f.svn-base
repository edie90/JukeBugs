  width: 100%;
  height: 100%;
  overflow: hidden;
}
.kerve .wave {
  display: block;
  position: absolute;
  width: 744px;
  height: 238px;
  top: 20px;
  right: 0;
  -webkit-transform: translate(4px, 0px);
  -moz-transform: translate(4px, 0px);
  -ms-transform: translate(4px, 0px);
  transform: translate(4px, 0px);
}
.kerve .wave_fade {
  display: block;
